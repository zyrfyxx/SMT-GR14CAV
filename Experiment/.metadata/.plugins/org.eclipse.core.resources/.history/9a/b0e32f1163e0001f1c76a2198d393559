spec ConstraintSolving
		
			
env boolean boolVar_1154123508;
env boolean boolVar_1744027187;
env boolean boolVar_54854665;
env boolean boolVar_535049014;





sys boolean ac_on;



// Init expressions Generated by ......


// Cores Generated by ......
asm ini !( boolVar_1744027187 & !boolVar_535049014 & !boolVar_1154123508 & boolVar_54854665 );
asm alw !( boolVar_1744027187 & !boolVar_535049014 & !boolVar_1154123508 & boolVar_54854665 );

gar ini ac_on = false;





gar pRespondsToS(boolVar_535049014, ac_on);
gar pRespondsToS(boolVar_1154123508, !ac_on);
gar pRespondsToS(boolVar_1744027187, ac_on);
gar pRespondsToS(boolVar_54854665, !ac_on);

pattern pRespondsToS(p, s) {
  var { S0, S1} state;

  -- initial assignments: initial state
  state=S0;

  -- safety this and next state
  G ((state=S0 & ((!p) | (p & s)) & next(state=S0)) |
  (state=S0 & (p & !s) & next(state=S1)) |
  (state=S1 & (s) & next(state=S0)) |
  (state=S1 & (!s) & next(state=S1)));

  -- equivalence of satisfaction
  GF (state=S0);
}